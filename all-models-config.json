  },
  "auth": {
    "profiles": {
      "gemini:backup": {
        "provider": "gemini",
        "mode": "api_key"
      },
      "anthropic:default": {
        "provider": "anthropic",
        "mode": "api_key"
      }
    },
    "order": {
      "gemini": ["gemini:backup"],
      "anthropic": ["anthropic:default"]
    }
  }
